{
    "name": "iamproperty/laravel-multiauth",
    "description": "Helpers for managing multiple authentication guards in Laravel",
    "type": "library",
    "authors": [
        {
            "name": "James Fenwick",
            "email": "james.fenwick@iamproperty.com"
        }
    ],
    "license": "proprietary",
    "require": {
        "illuminate/auth": "^5.6|^5.7|^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "IAMProperty\\MultiAuth\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IAMProperty\\MultiAuth\\MultiAuthServiceProvider"
            ]
        }
    }
}
